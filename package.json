{
  "name": "obsidian-vault-asciidoc-exporter",
  "version": "1.0.0",
  "description": "Export your entire Obsidian vault as AsciiDoc with preserved structure and diagrams",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.js",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.js production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "export",
    "asciidoc",
    "diagrams",
    "mermaid",
    "excalidraw"
  ],
  "author": "Harald Lapp",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/tar-stream": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "tar-stream": "^3.1.7"
  }
}
